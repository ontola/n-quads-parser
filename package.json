{
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*",
            "**/*.css",
            "**/*.ttl"
          ]
        }
      ],
      [
        "@pika/plugin-wasm-assemblyscript"
      ],
      [ "@pika/plugin-wasm-bindings" ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "name": "n-quads-parser",
  "version": "2.0.3",
  "description": "Simple, fast, n-quads and n-triples parser",
  "main": "dist/n-quads-parser.umd.js",
  "module": "dist/n-quads-parser.es6.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pika build",
    "prebuild": "rm -rf dist/*",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ontola/n-quads-parser.git"
  },
  "keywords": [
    "rdflib",
    "nquads",
    "ntriples",
    "nq",
    "parser",
    "rdfjs",
    "rdf",
    "linked",
    "data"
  ],
  "author": "Thom van Kalkeren <thom@argu.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ontola/n-quads-parser/issues"
  },
  "homepage": "https://github.com/ontola/n-quads-parser#readme",
  "files": [
    "dist/*",
    "package.json"
  ],
  "devDependencies": {
    "@ontologies/core": "=1.5.2-strict-20",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.7.1",
    "@pika/plugin-build-web": "^0.7.1",
    "@pika/plugin-standard-pkg": "^0.7.1",
    "@pika/plugin-wasm-assemblyscript": "^0.7.1",
    "@pika/plugin-wasm-bindings": "^0.7.1",
    "@types/jest": "^24.0.11",
    "assemblyscript": "^0.8.1",
    "jest": "^24.7.1",
    "rollup": "^1.27.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
